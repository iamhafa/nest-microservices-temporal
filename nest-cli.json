{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "generateOptions": {
    "spec": false
  },
  "compilerOptions": {
    "builder": "tsc",
    "deleteOutDir": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "contract": {
      "type": "library",
      "root": "libs/contract",
      "entryFile": "index",
      "sourceRoot": "libs/contract/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contract/tsconfig.lib.json"
      }
    },
    "inventory-service": {
      "type": "application",
      "root": "apps/inventory-service",
      "entryFile": "main",
      "sourceRoot": "apps/inventory-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory-service/tsconfig.app.json"
      }
    },
    "orchestrator-worker": {
      "type": "application",
      "root": "apps/orchestrator-worker",
      "entryFile": "main",
      "sourceRoot": "apps/orchestrator-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orchestrator-worker/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "payment-service": {
      "type": "application",
      "root": "apps/payment-service",
      "entryFile": "main",
      "sourceRoot": "apps/payment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-service/tsconfig.app.json"
      }
    },
    "product-service": {
      "type": "application",
      "root": "apps/product-service",
      "entryFile": "main",
      "sourceRoot": "apps/product-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-service/tsconfig.app.json"
      }
    },
    "shipping-service": {
      "type": "application",
      "root": "apps/shipping-service",
      "entryFile": "main",
      "sourceRoot": "apps/shipping-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shipping-service/tsconfig.app.json"
      }
    },
    "temporal": {
      "type": "library",
      "root": "libs/temporal",
      "entryFile": "index",
      "sourceRoot": "libs/temporal/src",
      "compilerOptions": {
        "tsConfigPath": "libs/temporal/tsconfig.lib.json"
      }
    }
  }
}